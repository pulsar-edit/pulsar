'.source.python':
  'editor':
    'autoIndentOnPaste': false
    'softTabs': true
    'tabLength': 4
    'commentStart': '# '
    'increaseIndentPattern': '^\\s*(class|def|elif|else|except|finally|for|if|try|with|while|async def|async for|async with)\\b.*:\\s*$'
    'decreaseIndentPattern': '^\\s*(elif|else|except|finally)\\b.*:\\s*$'
